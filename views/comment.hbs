    <header>
        <div class="topBar">
            <div class="logo">
                <a href="">
                    <img src="../res/logo.png" alt="logo">
                </a>
            </div>
            <div>
                <div class="topButton">
                    <a href="/dashboard/{{this._id}}">Dashboard</a>
                </div>
                <div class="topButton">
                    <a href="">Comment</a>
                    {{!-- TODO: href--}}
                </div>
                <img class="line" src="/res/Line.png" alt="line">
