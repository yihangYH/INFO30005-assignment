{{#with patientInfo}}
    <div class="main">
        <div class="header">
            <div class ="logo">
                <img class="logo" src="../../res/logo.png" alt="diabate">
            </div>
        </div>
    </div>
    <div class="main-body" id="main-body">
        <div class="laederboard">
            <div class="leaderboard-header">
                <p>Leaderboard</p>
            </div>
            <div class="leaderboard-rank">
                <img class="leaderboard-pic" src="../../res/leaderboard.png" alt="leaderboard image">
                <div class="rank-first">
                    <p>{{findRank this.rank 0}}</p>
                </div>
                <div class="rank-second">
                    <p>{{findRank this.rank 1}}</p>
                </div>
                <div class="rank-third">
                    <p>{{findRank this.rank 2}}</p>
                </div>
                <div class="rank-fourth-title">
                    <p>4th.</p>
                </div>
                <div class="rank-fourth">
                    <p>{{findRank this.rank 3}}</p>
                </div>
                <div class="rank-fifth-title">
                    <p>5th.</p>
                </div>
                <div class="rank-fifth">
                    <p>{{findRank this.rank 4}}</p>
                </div>     
            </div>
            <div class="leaderboard-comment">
                <p>Everyone keep going!</p>
            </div>
            <div class="middle-line"></div>
            <img class="back-icon" src="../../res/back.jpg" alt="back icon">
            <div class="achievement">
                <div class="achievement-header">
                    <p>Achievement</p>
                </div>
                <img class="achievement-image" src="../../res/trophie.png" alt="achievement-image">
                <div class="achievement-word">
                    <p>Your engagement rate is</p>
                </div>
                <div class="achievement-rate">
                    <p>{{this.rate}}%</p>
                </div>
                <div class="encourage-word">
                    <p>Good Job!!</p>
                </div>
                </div>
            </div>
        </div>        
    </div>


{{/with}}

