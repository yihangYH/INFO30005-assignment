                        </li>
                        <li > <input type="text" class="screenName" id="screenName" placeholder="Screen name" name="screen_name" readonly></li>
                        <li > <input type="text" class="brith" id="brith" placeholder="Year of birth                       DD/MM/YY" name="birthday" readonly></li>
                        <li > <input type="email" class="userId" id="userId" placeholder="User id" name="userid" readonly></li>
                        <li > <input type="text" class="password" id="password" placeholder="Password" name="password" readonly></li>
                    </ul>
